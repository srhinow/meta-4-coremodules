{
  "name": "srhinow/meta-4-coremodules",
  "description": "meta-settings for Contao-Core-modules like news,newsletter,calendar-events and faq",
  "keywords": ["contao", "keywords", "description", "pagetitle","meta","news","newsletter","events","faq","seo","og","twitter","google"],
  "type": "contao-module",
  "homepage": "https://github.com/srhinow/meta-4-coremodules",
  "license": "LGPL-3.0+",
  "authors":[
    {
      "name":"Sven Rhinow",
      "homepage":"https://www.sr-tag.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email": "kservice@sr-tag.de",
    "issues":"https://github.com/srhinow/meta-4-coremodules/issues",
    "source":"https://github.com/srhinow/meta-4-coremodules"
  },
  "require": {
    "php": ">=5.6",
    "contao/core-bundle": "~3.5 || ~4.4",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
  },
  "autoload":{
    "classmap": [
      "classes/"
    ]
  },
  "replace": {
    "contao-legacy/meta4coremodules": "self.version"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/meta4coremodules"
      }
    }
  }
}
